/// for smaller values

import java.util.*;
public class Test{
    
    public static void main(String args[]){
    Scanner sc=new Scanner(System.in);
    System.out.println("enter the number");
    int n=sc.nextInt();
    
    
    
    System.out.println("Fcatorial of the given number is:"+ fact(n));
    
}
    
    static int fact(int n){
        if(n==0){
            return 1;
            
        }
        
        return n*fact(n-1);
    }
    
    
    
}
